<view class="search"><top-bar vue-id="50cad900-1" isBorderBottom="{{true}}" bind:__l="__l" vue-slots="{{['center','add']}}"><view class="search_input" slot="center"><input class="uni-input" placeholder="搜索用户/群" data-event-opts="{{[['input',[['search',['$event']]]]]}}" bindinput="__e"/><view class="icon"><image src="../../static/images/index/search.png"></image></view></view><view data-event-opts="{{[['tap',[['close',['$event']]]]]}}" class="cancel" bindtap="__e" slot="add">取消</view></top-bar><view class="main"><view class="search-user result"><view hidden="{{!($root.g0>0)}}" class="title">用户</view><block wx:for="{{searchList}}" wx:for-item="user" wx:for-index="__i0__" wx:key="id"><view class="list user"><view class="left"><navigator url="/pages/userhome/userhome?id=aaa"><image class="left-avatar" src="{{user.imgurl}}" mode></image></navigator><view class="left-names"><view class="name"><rich-text nodes="{{user.name}}"></rich-text></view><view class="email"><rich-text nodes="{{user.email}}"></rich-text></view></view></view><block wx:if="{{user.tip===0}}"><view data-event-opts="{{[['tap',[['toUserhome',['$0'],[[['searchList','id',user.id]]]]]]]}}" class="right send" bindtap="__e">发消息</view></block><block wx:else><view data-event-opts="{{[['tap',[['toUserhome',['$0'],[[['searchList','id',user.id]]]]]]]}}" class="right add" bindtap="__e">加好友</view></block></view></block></view></view></view>